generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Subscriptions {
  id               String @id @default(uuid())
  nome             String
  telefone         String
  cpf              String
  cep              String
  endereco         String
  cidade           String
  estado           String
  cargo_atual      String
  cargo_pretendido String

  pixCode String?
  pago    Boolean @default(false)

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
